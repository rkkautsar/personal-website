{
  "private": true,
  "name": "rkkautsar",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --template src/template.html",
    "serve": "npm run -s build && preact serve",
    "dev": "preact watch --template src/template.html",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "airbnb"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^4.9.0",
    "if-env": "^1.0.0",
    "preact-cli": "^2.0.1",
    "prettier-eslint": "^8.8.1"
  },
  "dependencies": {
    "emotion": "^8.0.12",
    "preact": "^8.2.6",
    "preact-emotion": "^8.0.12",
    "preact-router": "^2.5.7",
    "webfontloader": "^1.6.28"
  }
}
