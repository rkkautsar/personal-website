{
  "private": true,
  "name": "rkkautsar",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --template src/template.html",
    "serve": "npm run -s build && preact serve",
    "dev": "preact watch --template src/template.html",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "airbnb"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.16.0",
    "if-env": "^1.0.4",
    "preact-cli": "^2.2.1",
    "preact-cli-plugin-critical-css": "^0.1.0",
    "prettier-eslint": "^9.0.0"
  },
  "dependencies": {
    "emotion": "^8.0.12",
    "preact": "^8.4.2",
    "preact-emotion": "^8.0.12",
    "preact-router": "^2.6.1",
    "react-ga": "^2.5.3",
    "webfontloader": "^1.6.28"
  }
}
